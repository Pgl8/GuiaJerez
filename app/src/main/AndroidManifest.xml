<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pgl8.guajerez" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activityPrincipal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityVinos"
            android:label="@string/title_activity_vinos"
            android:parentActivityName=".activityPrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activityPrincipal" />
        </activity>
        <activity
            android:name=".InfoVinosActivity"
            android:label="@string/title_activity_info_vinos"
            android:parentActivityName=".activityVinos" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pgl8.guajerez.activityVinos" />
        </activity>
    </application>

</manifest>
